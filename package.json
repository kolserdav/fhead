{
  "name": "file-headers",
  "version": "3.1.11",
  "main": "bin/fhead.js",
  "bin": {
    "fhead": "./bin/fhead.js"
  },
  "repository": "https://github.com/kolserdav/file-headers",
  "author": "kolserdav <serega12101983@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p . && node scripts/index.js env",
    "start": "node ${npm_package_main}",
    "dev:watch": "tsc -p . -w",
    "dev:start": "nodemon ${npm_package_main}"
  },
  "files": [
    "bin/fhead.js",
    "bin/main.js"
  ],
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "typescript": "^4.4.4"
  },
  "nodemonConfig": {
    "ignore": [
      "src"
    ]
  },
  "fhead": {
    "root": "src",
    "repository": "https://github.com/kolserdav/fhead.git",
    "patterns": [
      ".js",
      ".ts"
    ],
    "exclude": [
      "test"
    ],
